

#include <bits/stdc++.h>
#define ll long long
#define de double

using namespace std;

int main( )
{
    int H;
    int M;
    char colon;
    while(cin >> H >> colon >> M)
    {
        if(H == 0 and M == 0) break;

        de Theta_H = 30*H + 0.5*M;
        de Theta_M = 6*M;
        de Theta = abs(Theta_H - Theta_M);
        Theta = min(Theta, 360-Theta);

        cout << fixed << setprecision(3) << Theta << '\n';
    }
    return 0;
}

