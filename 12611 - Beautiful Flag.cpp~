#include <bits/stdc++.h>

#define ll long long
#define u unsigned
#define de double
#define pi acos(-1.0)
#define FF first
#define SS second
#define pb push_back
#define mp make_pair
#define mem(ae, value) memset(ae, value, sizeof(ae))
#define all(x) x.begin( ), x.end( )
#define UNIQUE(x) x.resize(unique(all(x)) - x.begin( ))
#define FASTER ios_base::sync_with_stdio(0)
#define loop(begin, end) for(u int i=begin; i<=end; i++)
#define Read freopen("in.txt", "r", stdin)
#define Write freopen("out.txt", "w", stdout)
#define nl '\n'
#define square(x) ((x)*(x))

using namespace std;

int main(int argc, char **argv)
{
    int T; cin >> T;
    int C=0;

    while(T --)
    {
        int r; cin >> r;
        de l = r*5.0;
        de w = r*3.0;

        de partition1 = l * 0.45;
        de partition2 = l * 0.55;

        C ++;
        cout << "Case " << C << ":" << nl;
        cout << fixed << setprecision(0) << -partition1 << " " << w/2 << nl;
        cout << fixed << setprecision(0) << partition2 << " " << w/2 << nl;
        cout << fixed << setprecision(0) << partition2 << " " << -w/2 << nl;
        cout << fixed << setprecision(0) << -partition1 << " " << -w/2 << nl;

    }

    return 0;
}

