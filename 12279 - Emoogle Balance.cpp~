

#include <bits/stdc++.h>

#define nl '\n'

using namespace std;

int main(int argc, char **argv)
{
    int Case = 1;
    int n;
    while((cin >> n), n)
    {
        int i=0; int C=0;
        while(i<n)
        {
            int value; cin >> value;
            if(value) C ++;
            else C --;

            i ++;
        }
        cout << "Case " << Case << ": " << C << nl;
        Case ++;
    }
    return 0;
}

